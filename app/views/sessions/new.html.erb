<div class="container">
  <h1>Login</h1>
  <%= form_with url: login_path, local: true do |f| %>
    <p>
      <%= f.label :email %><br>
      <%= f.email_field :email %>
    </p>
    <p>
      <%= f.label :password %><br>
      <%= f.password_field :password %>
    </p>
    <%= f.submit 'Login' %>
  <% end %>
  <p>New user? <%= link_to 'Register', register_path %></p>
</div>
<link rel="stylesheet" href="assets/welcome.css" />